{
	"name": "log-scope-ext",
	"displayName": "log-scope-ext",
	"description": "Formatted scope log extension",
	"icon": "icon.png",
	"version": "0.0.1",
	"publisher": "d",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:log-scope-ext.loginfo",
		"onCommand:log-scope-ext.logwarn",
		"onCommand:log-scope-ext.logerror"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "log-scope-ext.loginfo",
				"title": "loginfo"
			},
			{
				"command": "log-scope-ext.logwarn",
				"title": "logwarn"
			},
			{
				"command": "log-scope-ext.logerror",
				"title": "logerror"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
